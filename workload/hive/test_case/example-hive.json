[
    {
        "USE" : "example-conf.json"
    },
    {
        "SUMMARY" : {
            "median_sec" : [
                "<PMH>/workload/spark/scripts/spark_event_log_median_sec.pl",
                "<INFO>",
                "<TAG>"
            ],
            "spark_event_log" : [
                "<PMH>/workload/spark/scripts/spark_event_log_web_link.pl",
                "<INFO>",
                "<TAG>"
            ]
        }
    },
    {
        "TAG" : "simple_sql",
        "CMD" : {
            "COMMAND" : "<HIVE_HOME>/bin/hive",
            "PARAM" : [
                "--database tpcds_bin_partitioned_orc_1000",
                "-f <RESOURCE>/tpcds_hive_where2_queue1.sql"
            ]
        },
        "REPEAT" : 1,
        "DROP_CACHE_BETWEEN_REPEAT" : "FALSE"
    },
    {
        "ACTION" : "DROP_CACHE"
    },
    {
        "TAG" : "throughput_test",
        "CMD" : {
            "COMMAND" : "<RESOURCE>/test_multiple_queue.sh",
            "PARAM" : [
                "<RUNDIR>/test1"
            ]
        },
        "REPEAT" : 1,
        "DROP_CACHE_BETWEEN_REPEAT" : "FALSE"
    }
]
