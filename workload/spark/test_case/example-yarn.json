[
    {
        "USE" : "example-yarn-spark-conf.json"
    },
    {
        "SUMMARY" : {
            "median_sec" : [
                "<PMH>/workload/spark/scripts/spark_event_log_median_sec.pl",
                "<INFO>",
                "<TAG>"
            ],
            "spark_event_log" : [
                "<PMH>/workload/spark/scripts/spark_event_log_web_link.pl",
                "<INFO>",
                "<TAG>"
            ],
            "log_analysis" : [
                "<PMH>/workload/spark/scripts/spark_event_log_analysis.pl",
                "<INFO>",
                "<TAG>"
            ]
        }
    },
    {
        "TAG" : "q1_exec1_vcore2",
        "CMD" : {
            "COMMAND" : "<SPARK_HOME>/bin/run-example",
            "EXECUTOR_PER_DN" : 1,
            "EXECUTOR_VCORES" : 2,
            "EXECUTOR_MEM" : "1g",
            "DRIVER_MEM" : "1g",
            "PARAM" : [
                {
                    "--conf" : [
                        "spark.executor.extraJavaOptions=\\\"-XX:ParallelGCThreads=4 -XX:+AlwaysTenure\\\""
